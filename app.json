{
  "expo": {
    "name": "uizHub",
    "slug": "Earn-by-Playing",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "3.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devnity.quizhub"
    },
    "android": {
      "package": "com.devnity.quizhub"
    }
  }
}
